<div class="container w-50 flex-column justify-content-center align-items-center">

<ul>
  {{#each githubList.list}}
  <li>{{this}}</li>
  {{/each}}
</ul>

{{#each users as |user|}}
    <div class="row align-center mb-5">
      <div class="col-md-6">
        <h2>
          {{user.name}}
        </h2>
      </div>
      <div class="col-md-6">
        <h3>
          <a href="mailto:{{user.email}}">
            {{user.email}}
          </a>
        </h3>
      </div>
    </div>
  </div>
{{/each}}